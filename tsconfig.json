{
  "compilerOptions": {
    /* Language and Environment */
    "outDir": "./build",
    "noEmit": false,
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  },
  "ts-node": {
    "esm": true
  },
}