{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "airbnb-typescript",
        "airbnb-typescript/base"
    ],
    "plugins": [
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "import/extensions": [
            "error",
            "always"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}